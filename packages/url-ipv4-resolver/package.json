{
    "name": "@wallpaperdb/url-ipv4-resolver",
    "version": "0.0.1",
    "type": "module",
    "private": true,
    "description": "Package for resolving a url's domain name to an ipv4 address",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        }
    },
    "scripts": {
        "clean": "rm -rf dist",
        "format": "biome format --write .",
        "format:check": "biome format .",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "test": "vitest run",
        "test:unit": "vitest run --coverage",
        "test:watch": "vitest",
        "check-types": "tsc --noEmit"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^22.10.2",
        "nats": "^2.29.1",
        "typescript": "^5.7.2",
        "@biomejs/biome": "^2.3.4",
        "vitest": "^2.1.8"
    }
}
