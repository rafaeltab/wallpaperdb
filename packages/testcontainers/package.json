{
  "name": "@wallpaperdb/testcontainers",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "Shared testcontainer utilities for WallpaperDB tests",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./containers": {
      "import": "./dist/containers/index.js",
      "types": "./dist/containers/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "testcontainers": "^11.8.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "nats": "^2.29.1",
    "typescript": "^5.7.2",
    "@biomejs/biome": "^2.3.4",
    "vitest": "^2.1.8"
  }
}
